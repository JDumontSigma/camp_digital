<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="/scss/styles.css" type="text/css">
    <title>{{title}}</title>
</head>
<body>
    <section class="container">
         <!--First check to see if the browser supports canvas -->
         <!-- Background Colour -->
        <canvas width="1920" height="1080" id="backgroundCanvas">Your browser does not support canvas!</canvas>
        <!-- Title of the piece  -->
        <canvas width="1000" height="150" id="title"></canvas>
        <!-- Heart Image Rendering  -->
        <canvas width="500" height="500" id="heartBeat"></canvas>
        <!-- Actually tweets  -->
        <canvas width="450" height="1080" id="twitterFeed"></canvas>
        <!-- Number of tweets counter   -->
        <canvas width="400" height="400" id="tweetCount"></canvas>
        <!-- Number of followers counter   -->
        <canvas width="500" height="400" id="reachCount"></canvas>
        <!-- Last five tweeters   -->
        <canvas width="600" height="700" id="highTweet"></canvas>
        <!-- Last five tweeters   -->
        <canvas width="400" height="400" id="lastFive"></canvas>
         <!-- Progressive Chart   -->
        <canvas width="1056" height="280" id="chart"></canvas>
         <!-- Last Update  -->
        <canvas width="400" height="200" id="lastUpdate"></canvas>
    </section>
    <section class="fullScreen">Go Full Screen!</section>
    <!--Images rendered here waiting to be drawn to canvas -->
    <section class="heartImages">
        <img src="../img/header_bg.png" width="512" height='96' alt="banner_bg" id="banner">
        <img src="../img/heart-1.png" id="heart_1">
        <img src="../img/heart-2.png" id="heart_2">
        <img src="../img/heart-3.png" id="heart_3">
        <img src="../img/heart-4.png" id="heart_4">
        <img src="../img/heart-5.png" id="heart_5">
        <img src="../img/heart-6.png" id="heart_6">
        <img src="../img/heart-7.png" id="heart_7">
        <img src="../img/heart-8.png" id="heart_8">
        <img src="../img/heart-9.png" id="heart_9">
        <img src="../img/heart-10.png" id="heart_10">
        <img src="../img/heart-11.png" id="heart_11">
        <img src="../img/heart-12.png" id="heart_12">
        <img src="../img/heart-13.png" id="heart_13">
        <img src="../img/heart-14.png" id="heart_14">
        <img src="../img/heart-15.png" id="heart_15">
        <img src="../img/heart-16.png" id="heart_16">
        <img src="../img/heart-17.png" id="heart_17">
        <img src="../img/heart-18.png" id="heart_18">
        <img src="../img/heart-19.png" id="heart_19">
        <img src="../img/heart-20.png" id="heart_20">
        <img src="../img/heart-21.png" id="heart_21">
        <img src="../img/heart-22.png" id="heart_22">
        <img src="../img/heart-23.png" id="heart_23">
        <img src="../img/heart-24.png" id="heart_24">
        <img src="../img/heart-25.png" id="heart_25">
    </section>
</body>
<script src="/js/scripts.bundled.js"></script>
</html> 